<div class="well">
    <%= link_to "Добавить документ", new_document_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-document"></div>
  
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Название</th>
        <th>Описание</th>
        <th>Ссылка</th>

        <td>&nbsp;</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody class="document-index">
      <%= render "index" %>
    </tbody>
  </table>
<div id="document-modal" class="modal fade">
<div class="modal-dialog">
<div id="inner-document-modal" class="modal-content"></div>
</div>
</div>